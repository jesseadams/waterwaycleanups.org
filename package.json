{
  "name": "waterways-cleanup",
  "version": "1.0.0",
  "description": "Waterways Cleanup Project",
  "scripts": {
    "dev": "concurrently \"hugo server\" \"npm run watch:assets\"",
    "build": "npm run build:assets && hugo --minify && npm run build:merch",
    "build:assets": "webpack --mode production",
    "watch:assets": "webpack --watch --mode development",
    "clean": "rm -f static/css/tailwind-output.css static/js/main-bundle.js",
    "start": "npm run build && hugo server",
    "build:merch": "npx esbuild merch-page/index.tsx --bundle --outfile=static/merch-page/app.js --format=esm --platform=browser --target=es2020 --jsx=automatic --jsx-import-source=react --external:react --external:react-dom/client --external:react/jsx-runtime --external:@stripe/stripe-js --external:sonner --minify",
    "sync-products": "node scripts/sync-products.js && npm run build:merch",
    "sync-products:dry-run": "node scripts/sync-products.js --dry-run",
    "sync-products:verbose": "node scripts/sync-products.js --verbose && npm run build:merch",
    "products": "node scripts/product-manager.js",
    "fix-rsvps": "node scripts/fix-rsvp-event-ids.js --dry-run --environment=staging",
    "fix-rsvps:staging": "node scripts/fix-rsvp-event-ids.js --environment=staging",
    "fix-rsvps:prod": "node scripts/fix-rsvp-event-ids.js --environment=prod",
    "fix-brooke-road": "node scripts/fix-brooke-road-rsvps.js --dry-run --environment=staging",
    "fix-brooke-road:staging": "node scripts/fix-brooke-road-rsvps.js --environment=staging",
    "fix-brooke-road:prod": "node scripts/fix-brooke-road-rsvps.js --environment=prod",
    "generate-hugo": "node scripts/hugo-generator.js --environment=staging",
    "generate-hugo:dry-run": "node scripts/hugo-generator.js --dry-run --environment=staging",
    "generate-hugo:prod": "node scripts/hugo-generator.js --environment=prod",
    "generate-hugo:verbose": "node scripts/hugo-generator.js --verbose --environment=staging",
    "test-hugo-generator": "node scripts/test-hugo-generator.js",
    "migrate": "node scripts/run-full-migration.js --environment=staging",
    "migrate:dry-run": "node scripts/run-full-migration.js --dry-run --environment=staging --verbose",
    "migrate:prod": "node scripts/run-full-migration.js --environment=prod",
    "migrate:prod:dry-run": "node scripts/run-full-migration.js --dry-run --environment=prod --verbose",
    "migrate:validate": "node scripts/validate-migration.js --environment=staging --verbose",
    "migrate:validate:prod": "node scripts/validate-migration.js --environment=prod --verbose",
    "migrate:rsvps": "node scripts/migrate-rsvp-data.js --environment=staging",
    "migrate:rsvps:dry-run": "node scripts/migrate-rsvp-data.js --dry-run --environment=staging --verbose",
    "migrate:status": "node scripts/migration-status.js --environment=staging",
    "migrate:status:prod": "node scripts/migration-status.js --environment=prod",
    "migrate:check": "node scripts/check-migration-needed.js --environment=staging",
    "migrate:check:prod": "node scripts/check-migration-needed.js --environment=prod",
    "admin-server": "node scripts/admin-events-server.js",
    "deploy:database": "node scripts/deploy-with-database.js --environment=staging",
    "deploy:database:prod": "node scripts/deploy-with-database.js --environment=prod",
    "deploy:database:force": "node scripts/deploy-with-database.js --environment=staging --force-migration",
    "deploy:database:verbose": "node scripts/deploy-with-database.js --environment=staging --verbose",
    "validate:deployment": "node scripts/validate-deployment.js --environment=staging",
    "validate:deployment:prod": "node scripts/validate-deployment.js --environment=prod"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.826.0",
    "@tailwindcss/cli": "^4.1.8",
    "@tailwindcss/postcss": "^4.1.8",
    "aws-sdk": "^2.1693.0",
    "js-yaml": "^4.1.1",
    "lineicons-react": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simple-parallax-js": "^5.6.2",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^10.0.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "daisyui": "^5.0.43",
    "dotenv": "^17.2.2",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "stripe": "^18.5.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^4.1.8",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}
