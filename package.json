{
  "name": "waterways-cleanup",
  "version": "1.0.0",
  "description": "Waterways Cleanup Project",
  "scripts": {
    "dev": "concurrently \"HUGO_ENV=development hugo server\" \"npm run watch:assets\"",
    "build": "NODE_ENV=production npm run build:assets && HUGO_ENV=production hugo --minify",
    "build:assets": "webpack --mode production",
    "watch:assets": "NODE_ENV=development webpack --watch --mode development",
    "minify:css": "cleancss -o static/css/style.min.css static/css/style.css",
    "clean": "rm -f static/css/tailwind-output*.css static/css/style.min.css static/js/*-bundle.js",
    "serve": "HUGO_ENV=production hugo server",
    "start": "npm run build && npm run serve"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.8",
    "@tailwindcss/postcss": "^4.1.8",
    "lineicons-react": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simple-parallax-js": "^5.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^10.0.0",
    "clean-css-cli": "^5.6.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cssnano": "^7.0.7",
    "daisyui": "^5.0.43",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.4",
    "tailwindcss": "^4.1.8",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  }
}
